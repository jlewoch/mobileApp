{
  "name": "mobileapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^0.19.0",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-calendar-picker": "^6.0.1",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-image-picker": "^1.1.0",
    "react-native-reanimated": "^1.3.0",
    "react-native-screens": "^2.0.0-alpha.5",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.1",
    "react-navigation-stack": "^1.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/runtime": "7.6.3",
    "@react-native-community/eslint-config": "0.0.3",
    "babel-jest": "24.9.0",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "6.5.1",
    "jest": "24.9.0",
    "jest-enzyme": "^7.1.1",
    "jsdom": "^15.2.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-dom": "^16.10.2",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>setup-tests.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-gesture-handler|react-native-vector-icons|react-navigation|react-navigation-stack|react-native-screens|@react-navigation|react-native-calendar-picker|react-native-image-picker)/)"
    ]
  }
}
